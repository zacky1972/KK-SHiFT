{
  "name": "kk-shift",
  "version": "1.0.0",
  "description": "for KK-Shift",
  "main": "index.js",
  "dependencies": {
    "bootstrap": "^4.0.0-beta.2",
    "jquery": "^3.2.1",
    "popper.js": "^1.12.7",
    "scroll-depth": "^1.0.0"
  },
  "devDependencies": {
    "browserify": "^14.5.0",
    "browserify-shim": "^3.8.14",
    "gulp": "^3.9.1",
    "gulp-sass": "^3.1.0",
    "node-neat": "^2.0.0-beta.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "scroll-depth": "./node_modules/scrool-depth/jquery.scrool-depth.js",
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js",
    "popper.js": "./node_modules/popper.js/dist/umd/popper.js",
    "jquery": "./node_modules/jquery/dist/jquery.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "popper.js": "$",
    "bootstrap": {
      "exports": "$",
      "depends": [
        "jquery:jQuery",
        "popper.js:Popper.js"
      ]
    },
    "scrool-depth": {
      "exports": "$",
      "depends": [
        "jquery:jQuery"
      ]
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zacky1972/KK-Shift.git"
  },
  "author": "Susumu Yamazaki",
  "bugs": {
    "url": "https://github.com/zacky1972/KK-Shift/issues"
  },
  "homepage": "https://github.com/zacky1972/KK-Shift#readme"
}
